{
  "rewrites": [
    {
      "source": "/dashboard/assets/(.*)",
      "destination": "/dashboard/dist/assets/$1"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/dashboard/dist/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}


